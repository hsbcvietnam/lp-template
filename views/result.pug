doctype html
html(lang='en')
  head
    title Result
    style.
      table {
        border-collapse: collapse;
      }
      td {
        padding: 5px 15px;
        border: 1px solid #000;
      }
  body
    h1 Campaign Name: #{campaign}
    table
      each value, key in data
        tr
          td!= key
          td!= value


